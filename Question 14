#include "pch.h"
#include <iostream>
#include <math.h>
using namespace std;

int main()
{
	double x1;
	double x2;
	double y1;
	double y2;
	double d;

	cout << "Enter x1 ";
	cin >> x1;
	cout << "Enter x2 ";
	cin >> x2;
	cout << "Enter y1 ";
	cin >> y1;
	cout << "Enter y2 ";
	cin >> y2;

	d = sqrt(pow(x2 - x1,2) + pow(y2 - y1,2));

	cout << "The distance is " << d << endl;

	return 0;
}
